{
  "name": "mll-lab/php-cs-fixer-config",
  "type": "library",
  "description": "Shared rules for php-cs-fixer",
  "keywords": [
    "php",
    "php-cs-fixer"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Benedikt Franke",
      "email": "benedikt@franke.tech"
    }
  ],
  "require": {
    "php": "^7.4",
    "friendsofphp/php-cs-fixer": "^2"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.13",
    "phpunit/phpunit": "^8",
    "sanmai/phpunit-legacy-adapter": "^6 || ^8"
  },
  "autoload": {
    "files": [
      "config.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
